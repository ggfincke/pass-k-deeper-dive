OLLAMA_URL=http://localhost:11434   # URL of the local Ollama server
MODEL=gpt-oss:20b                   # Default Ollama model to target
N_SAMPLES=100                       # Number of completions generated per task
EVAL_KS=1,5,10,25                   # Evaluation k-values used for pass@k
CONCURRENCY=5                       # Concurrent workers for generation
MAX_EVAL_WORKERS=8                  # Max evaluation workers (cap at CPU count)
MAX_EXECUTION_TIMEOUT=30.0          # Per-task execution timeout in seconds
PASSK_TEMPERATURE=0.2               # Temperature override for sampling
MAX_NEW_TOKENS=131072               # Upper bound on new tokens per completion
MAX_RETRIES=2                       # Retry attempts when generation is empty
EMPTY_COMPLETION_MAX_RETRIES=2      # Override retry budget for empty recoveries (defaults to MAX_RETRIES)
EMPTY_COMPLETION_BACKOFF_BASE=0.1   # Initial backoff in seconds between empty retries
EMPTY_COMPLETION_BACKOFF_MAX=1.0    # Maximum backoff between empty retries
HUMAN_EVAL_MAX_PROCESSES=           # Optional cap on concurrent sandbox processes (defaults to min(cpu_count, 32))
TOP_P=1.0                           # Nucleus sampling parameter
TOP_K=0                             # Top-k cutoff (0 disables)
REPEAT_PENALTY=1.0                  # Penalty for repeating tokens
STOP=                               # Comma-separated stop sequences
OLLAMA_MAX_PARALLEL_REQUESTS=5      # Limit of concurrent Ollama HTTP requests
OLLAMA_HTTP_TIMEOUT=120.0           # Client-side timeout per Ollama call in seconds
OLLAMA_HTTP_MAX_RETRIES=3           # Retry budget for 429/503 Ollama responses
OLLAMA_RETRY_BASE_DELAY=0.1         # Initial retry backoff for throttled Ollama calls (seconds)
OLLAMA_RETRY_MAX_DELAY=2.0          # Upper bound on Ollama retry backoff (seconds)
